<div class="ajs-end-of-video-call-to-action-container">
	<div class="ajs-related-videos" data-squery="min-width:400px=wide max-width:10em=small">
		<h2>{model.relatedVideos.title}</h2>
		<div class='ajs-related-video-master'>
			{#model.relatedVideos.items}
			<a href="{link}">
				<div class='ajs-related-video-container'>
					<img alt='{title}' src='{img}'>
					<p>{title}</p>
				</div>
			</a>
			{/model.relatedVideos.items}
		</div>
	</div>	
</div>